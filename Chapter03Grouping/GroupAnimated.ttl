  x3do:hasParent :Appearance_2_4_2_1 ;
  x3do:DEF 'Pulsar' ;
  x3do:diffuseColor ( 0 0 1 ) .
:Sphere_2_4_2_2 a owl:NamedIndividual, x3do:Sphere ;
  x3do:hasParent :Shape_2_4_2 ;
