    <meta content='X3D-Edit, https://savage.nps.edu/X3D-Edit' name='generator'/>
    <meta content='../license.html' name='license'/>
  </head>
  <Scene>
    <Transform>
      <Sound DEF='Audible' direction='0 1 0' intensity='1.0' location='0 0 0' maxBack='50' maxFront='100' minBack='10' minFront='10' priority='1.' spatialize='true'>
        <AudioClip DEF='Water' description='Running Water' loop='true' startTime='1' stopTime='0' url='"aqua.wav" "http://realism.com/Web3D/Examples/Audio/aqua.wav"'/>
      </Sound>
    </Transform>
  </Scene>
</X3D>